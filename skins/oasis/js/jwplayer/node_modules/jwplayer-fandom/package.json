  },
  "scripts": {
    "build": "npm run copy-html && npm run build-all-css && npm run build-js && npm run build-i18n",
    "build-all-css": "npm run build-css && npm run build-dev-css",
    "build-css": "node-sass src/styles/index.scss --output-style compressed | postcss -u autoprefixer --autoprefixer.browsers=\"ie >= 11, Safari >= 7.1, last 2 versions\" > dist/index.css",
    "build-dev-css": "node-sass src/styles/dev.scss --output-style compressed | postcss -u autoprefixer --autoprefixer.browsers=\"ie >= 11, Safari >= 7.1, last 2 versions\" > dist/dev.css",
    "build-i18n": "node scripts/i18n-build.es6",
    "build-js": "uglifyjs --compress --mangle -o dist/wikiajwplayer.js --wrap wikiaJWPlayer -- src/*.js",
    "copy-html": "mkdir -p dist && cp src/*.html dist/",
    "dev": "npm run build && npm-run-all --parallel watch-other watch-js watch-scss watch-i18n server",
    "server": "http-server -c-1 dist",
    "watch-i18n": "nodemon --watch src/locales -e json --ignore dist/ -x \"npm run build-i18n\"",
    "watch-js": "nodemon -e js --ignore dist/ -x \"npm run build-js\"",
    "watch-other": "nodemon -e html --ignore dist/ -x \"npm run copy-html\"",
    "watch-scss": "nodemon -e scss -x \"npm run build-all-css\""
  },
  "version": "1.0.0"
}
