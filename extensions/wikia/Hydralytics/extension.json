{
	"name": "Hydralytics",
	"version": "2.1.2",
	"author": [
		"Brent Copeland",
		"Alexia E. Smith",
		"Hydra Wiki Platform Team"
	],
	"license-name": "GPL-2.0-or-later",
	"descriptionmsg": "hydralytics_description",
	"type": "specialpage",
	"requires": {
		"MediaWiki": ">= 1.29.0",
		"extensions": {
			"Cheevos": ">= 1.0.0",
			"DynamicSettings": ">= 3.0.0",
			"SyncService": ">= 2.0.0"
		}
	},
	"APIModules": {
		"hydralytics": "HydralyticsApi"
	},
	"AvailableRights": [
		"analytics"
	],
	"GroupPermissions": {
		"hydra_admin": {
			"analytics": true
		},
		"hydra_staff": {
			"analytics": true
		},
		"sysop": {
			"analytics": true
		}
	},
	"SpecialPages": {
		"Analytics": "Hydralytics\\SpecialAnalytics"
	},
	"MessagesDirs": {
		"Hydralytics": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"SpecialHydralytics": "Hydralytics.alias.php"
	},
	"AutoloadClasses": {
		"GetStats": "classes/GetStats.php",
		"HydralyticsApi": "classes/HydralyticsApi.php",
		"HydralyticsHooks": "Hydralytics.hooks.php",
		"Hydralytics\\DailyAnalyticsJob": "classes/jobs/DailyAnalyticsJob.php",
		"Hydralytics\\GoogleAnalytics": "classes/GoogleAnalytics.php",
		"Hydralytics\\Information": "classes/Information.php",
		"Hydralytics\\SiteStatsDailyJob": "classes/jobs/SiteStatsDailyJob.php",
		"Hydralytics\\SiteStatsHourlyJob": "classes/jobs/SiteStatsHourlyJob.php",
		"Hydralytics\\SpecialAnalytics": "specials/SpecialAnalytics.php",
		"Hydralytics\\TemplateAnalytics": "templates/TemplateAnalytics.php"
	},
	"ResourceModules": {
		"ext.hydralytics.styles": {
			"styles": [
				"css/hydralytics.less"
			]
		},
		"ext.hydralytics.scripts": {
			"scripts": [
				"js/chart.bundle.min.js",
				"js/moment.min.js",
				"js/lodash.min.js",
				"js/hydralytics.js"
			],
			"dependencies": [
				"mediawiki.language"
			],
			"messages": [
				"total_page_views",
				"total_visitors",
				"returning_visitors",
				"new_visitors",
				"based_on_last_30",
				"edits",
				"logged_in",
				"logged_out",
				"page_views",
				"user",
				"date",
				"engaged_admins",
				"nonengaged_admins"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "Hydralytics"
	},
	"Hooks": {
		"LoadExtensionSchemaUpdates": "HydralyticsHooks::onLoadExtensionSchemaUpdates"
	},
	"config": {
		"CommunityManager": {
			"value": "",
			"path": false,
			"discriptionmsg": "hydralytics-config-communitymanager",
			"public": false
		},
		"HLFaqUrl": {
			"value": "",
			"path": false,
			"descriptionmsg": "hydralytics-config-hlfaqurl",
			"public": false
		},
		"HLFeedbackUrl": {
			"value": "",
			"path": false,
			"descriptionmsg": "hydralytics-config-hlfeedbackurl",
			"public": false
		},
		"HLSlackUrl": {
			"value": "",
			"path": false,
			"descriptionmsg": "hydralytics-config-hlslackurl",
			"public": false
		},
		"StripGAProperties": {
			"value": [],
			"path": false,
			"descriptionmsg": "hydralytics-config-stripgaproperties",
			"public": false,
			"merge_strategy": "array_merge"
		},
		"SyncServices": {
			"value": {
				"DailyAnalyticsJob": "Hydralytics\\DailyAnalyticsJob",
				"SiteStatsDailyJob": "Hydralytics\\SiteStatsDailyJob",
				"SiteStatsHourlyJob": "Hydralytics\\SiteStatsHourlyJob"
			},
			"path": false,
			"descriptionmsg": "hydralytics-config-syncservices",
			"public": false,
			"merge_strategy": "array_merge"
		}
	},
	"load_composer_autoloader": true,
	"manifest_version": 2
}